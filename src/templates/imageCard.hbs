{{#each this}}
<div class="photo-card">
    <img src={{webformatURL}} alt="{{tags}}" class="js-ligthbox"/>
    <div class="stats">
        <p class="stats-item">
            <i class="material-icons gray08">thumb_up</i>
            {{likes}}
        </p>
        <p class="stats-item">
            <i class="material-icons gray08">visibility</i>
            {{views}}
        </p>
        <p class="stats-item">
            <i class="material-icons gray08">comment</i>
            {{comments}}
        </p>
        <p class="stats-item">
            <i class="material-icons gray08">cloud_download</i>
            {{downloads}}
        </p>
    </div>
</div>
{{/each}}